    &lt;/tr&gt;
  &lt;/thead&gt;
  &lt;tbody&gt;
    &lt;tr&gt;
      &lt;td&gt;iOS2&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;18.247&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;2%&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;iOS3&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;130.349&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;12%&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;iOS4&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;277.334&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;25%&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;iOS5&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;263.517&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;24%&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;iOS6&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;266.598&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;24%&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;iOS7&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;154.222&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;14%&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;iOS8&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;10.536&lt;/td&gt;
